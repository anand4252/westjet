   <xs:complexType name="NumberOfAssocProfilesType">
      <xs:annotation>
         <xs:documentation>Number of profiles associated to and from this profile</xs:documentation>
      </xs:annotation>
      <xs:attribute name="Traveler" type="NumericStringLength1to6" use="optional"/>
      <xs:attribute name="TravelAgent" type="NumericStringLength1to6" use="optional"/>
      <xs:attribute name="TravelAgency" type="NumericStringLength1to6" use="optional"/>
      <xs:attribute name="Corporation" type="NumericStringLength1to6" use="optional"/>
      <xs:attribute name="Operational" type="NumericStringLength1to6" use="optional"/>
      <xs:attribute name="GroupProfile" type="NumericStringLength1to6" use="optional"/>
   </xs:complexType>
