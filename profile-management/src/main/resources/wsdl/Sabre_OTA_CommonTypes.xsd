   <xs:complexType name="BldgRoomType">
      <xs:simpleContent>
         <xs:extension base="StringLength0to32">
            <xs:attribute name="FloorWing" type="StringLength1to5" use="optional"/>
            <xs:attribute name="Room" type="StringLength1to20" use="optional"/>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>
   <xs:complexType name="AddressType">
      <xs:annotation>
         <xs:documentation xml:lang="en">Provides address information.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="AddressLine" type="StringLength0to100" minOccurs="0" maxOccurs="4">
            <xs:annotation>
               <xs:documentation xml:lang="en">When the address is unformatted (FormattedInd="false") these lines will contain free form address details. When the address is formatted and street number and street name must be sent independently, the street number will be sent using StreetNmbr, and the street name will be sent in the first AddressLine occurrence.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="MailStop" type="StringLength1to16" minOccurs="0">
            <xs:annotation>
               <xs:documentation xml:lang="en">Company Mail Stop/Drop</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="POBox" type="StringLength1to16" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Post-office box.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="CityName" type="StringLength1to60" minOccurs="0">
            <xs:annotation>
               <xs:documentation xml:lang="en">City (e.g., Dublin), town, or postal station (i.e., a postal service territory, often used in a military address).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PostalCd" type="StringLength1to16" minOccurs="0">
            <xs:annotation>
               <xs:documentation xml:lang="en">Post Office Code number.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="StateCode" type="StringLength1to3" minOccurs="0">
            <xs:annotation>
               <xs:documentation xml:lang="en">State or Province Code based on Postal Information(e.g., TX for Texas).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="CountryCode" type="StringLength1to3" minOccurs="0">
            <xs:annotation>
               <xs:documentation xml:lang="en">Country name as a Code (e.g., US or USA for United States of America).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="StreetNmbr" type="StringLength1to64" minOccurs="0">
            <xs:annotation>
               <xs:documentation xml:lang="en">Street Name and Number within the address</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="BldgRoom" type="BldgRoomType" minOccurs="0">
            <xs:annotation>
               <xs:documentation xml:lang="en">Building name, room, apartment, or suite number. Attributes for floor/wing description, room description, and mail stop</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="InformationText" type="StringLength1to100" minOccurs="0">
            <xs:annotation>
               <xs:documentation xml:lang="en">In case when CountryCode or StateCode can't be mached with dictionary, this data can be stored in free text in this field.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DataSource" type="DataSourceInfoType" minOccurs="0">
            <xs:annotation>
               <xs:documentation xml:lang="en">*** SABRE ADDED *** Deprecated, will be removed in 7.0 release</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="TransactionalData" type="TransactionalDataType" minOccurs="0">
            <xs:annotation>
               <xs:documentation xml:lang="en">*** SABRE ADDED *** Transactional Data only , This data will not be stored with a profile, this will be stored only in templates if needed and should be used with Templates only</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ContactRemark" type="StringLength1to200" minOccurs="0">
            <xs:annotation>
               <xs:documentation>*** SABRE ADDED *** Additional information about the contact person.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="LocationTypeCode" type="StringLength1to3" use="optional">
         <xs:annotation>
