<xs:schema targetNamespace="http://www.sabre.com/eps/schemas" elementFormDefault="qualified" version="${version}" id="SABRE2007_OTA2007B" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.sabre.com/eps/schemas" xmlns:p3="http://www.sabre.com/eps/schemas">
   <xs:include schemaLocation="Sabre_OTA_SimpleTypes.xsd"/>
   <xs:include schemaLocation="Sabre_OTA_CommonTypes.xsd"/>
   <xs:include schemaLocation="Sabre_OTA_Common_Preferences.xsd"/>
   <xs:include schemaLocation="Sabre_OTA_Common_TPA_Extensions.xsd"/>
   <xs:include schemaLocation="Sabre_OTA_Profile_TPA_Identity.xsd"/>
   <xs:include schemaLocation="Sabre_OTA_Common_AgencyTypes.xsd"/>
   <xs:include schemaLocation="Sabre_OTA_Common_CorporationTypes.xsd"/>
   <xs:include schemaLocation="Sabre_OTA_Common_AgentTypes.xsd"/>
   <xs:include schemaLocation="Sabre_OTA_Common_TravelerTypes.xsd"/>
   <xs:include schemaLocation="Sabre_OTA_Common_OperationalProfileTypes.xsd"/>
   <xs:include schemaLocation="Sabre_OTA_Common_GroupProfileTypes.xsd"/>
   <xs:annotation>
      <xs:documentation xml:lang="en">All Schema files in the OTA specification are made available according to the terms defined by the OTA License Agreement at http://www.opentravel.org/ota_downloads_form.cfm. It contains Sabre modifications for EPS project. Added elements are described as *SABRE ADDED*</xs:documentation>
   </xs:annotation>
   <xs:complexType name="ProfileInfoType">
      <xs:choice>
         <xs:element name="Profile" type="ProfileType">
            <xs:annotation>
               <xs:documentation xml:lang="en">*** OTA Based *** Complete Update of a Profile</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PartialUpdates" type="PartialUpdatesType">
            <xs:annotation>
               <xs:documentation>*** OTA Based *** Partial Update of Profile</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:choice>
   </xs:complexType>
   <xs:complexType name="DupChkRSProfileInfoType">
      <xs:sequence>
         <xs:element name="Profile" type="DupChkRSProfileType" maxOccurs="25">
            <xs:annotation>
               <xs:documentation xml:lang="en">*** OTA Based *** Complete Update of a Profile</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="PartialUpdatesType">
      <xs:annotation>
         <xs:documentation>subtree to support partial updates</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="TPA_Identity" type="TPAIdentityType">
            <xs:annotation>
               <xs:documentation>*** SABRE ADDED *** Identity of a profile</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Delete" minOccurs="0">
            <xs:annotation>
               <xs:documentation>*** SABRE ADDED *** partial Delete of a profile</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="DeleteSubtree" type="DeleteSubtreeType" minOccurs="0" maxOccurs="200">
                     <xs:annotation>
                        <xs:documentation>*** SABRE ADDED *** partial Delete Sub Tree of a profile</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="DeleteElement" type="DeleteElementType" minOccurs="0" maxOccurs="2">
                     <xs:annotation>
                        <xs:documentation>*** SABRE ADDED *** Partial Delete Elements of a profile</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="Add" minOccurs="0">
            <xs:annotation>
               <xs:documentation>*** SABRE ADDED *** Partial Add data to a profile</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="AddSubtree" type="AddSubtreeType" minOccurs="0" maxOccurs="200">
                     <xs:annotation>
                        <xs:documentation>*** SABRE ADDED *** Partial Add Sub Tree to a profile</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="AddElement" type="AddElementType" minOccurs="0" maxOccurs="2">
                     <xs:annotation>
                        <xs:documentation>*** SABRE ADDED *** Partial Add Elements to a profile</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="Modify" minOccurs="0">
            <xs:annotation>
               <xs:documentation>*** SABRE ADDED *** Partial Modify data to a profile</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="ModifySubtree" type="ModifySubtreeType" minOccurs="0" maxOccurs="200">
                     <xs:annotation>
                        <xs:documentation>*** SABRE ADDED *** Partial Modify Sub Tree to a profile</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="ModifyElement" type="ModifyElementType" minOccurs="0" maxOccurs="2">
                     <xs:annotation>
                        <xs:documentation>*** SABRE ADDED *** Partial Modify Elements to a profile</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="DeleteElementType">
      <xs:annotation>
         <xs:documentation>Limited to schema elements that have no child elements and that are at the level of Profile/Customer/xxxx like Email or TPA_ProfileExtensions/xxxx like SpecialDat.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="MatchData" type="PartialUpdateElementsMatchDataType"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="DeleteSubtreeType">
      <xs:annotation>
         <xs:documentation>Limited to schema subtrees that are at the level of Profile/Customer/xxxxx like PaymentForm or TPA_ProfileExtensions/xxxx like BusinessTraveler NOTE: PersonName cannot be deleted.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="MatchSubtree" type="PartialUpdateProfileType">
            <xs:annotation>
               <xs:documentation>Requirement for min 1 instance for general case of subtrees with multiple instances. For subtrees with a single instance like Customer matching data not required</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="IgnoreMatchAttribute" type="PartialUpdateIgnoreAttributesType" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Attributes that will be ignored during element matching</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="child" type="PartialChildDataType" use="required"/>
   </xs:complexType>
   <xs:complexType name="ModifyElementType">
      <xs:annotation>
         <xs:documentation>Limited to schema elements that have no child elements and that are at the level of Profile/Customer/xxxx like Email or TPA_ProfileExtensions/xxxx like SpecialDat.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="MatchData" type="PartialUpdateElementsMatchDataType"/>
         <xs:element name="NewData" type="PartialUpdateElementsNewDataType"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ModifySubtreeType">
      <xs:annotation>
         <xs:documentation>Limited to schema subtrees that are at the level of Profile/Customer/xxxxx like PaymentForm or TPA_ProfileExtensions/xxxx like BusinessTraveler NOTE: PersonName cannot be deleted.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="MatchSubtree" type="PartialUpdateProfileType">
            <xs:annotation>
               <xs:documentation>Requirement for min 1 instance for general case of subtrees with multiple instances. For subtrees with a single instance like VITCustomer matching data not required. If no match found for criteria then NewSubtree data is created</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="IgnoreMatchAttribute" type="PartialUpdateIgnoreAttributesType" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Attributes that will be ignored during element matching</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="NewSubtree" type="PartialUpdateProfileType">
            <xs:annotation>
               <xs:documentation>Requires as much data as the view of the client has</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="child" type="PartialChildDataType" use="required"/>
      <xs:attribute name="MergeSubtree" type="YesNoType" use="optional" default="Y"/>
      <xs:attribute name="AddIfNotFound" type="YesNoType" use="optional" default="N"/>
   </xs:complexType>
   <xs:complexType name="AddElementType">
      <xs:annotation>
         <xs:documentation>Limited to schema elements that have no child elements and that are at the level of Profile/Customer/xxxx like Email or TPA_ProfileExtensions/xxxx like SpecialDat.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="NewData" type="PartialUpdateElementsNewDataType"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="AddSubtreeType">
      <xs:annotation>
         <xs:documentation>Limited to schema subtrees that are at the level of Profile/Customer/xxxxx like PaymentForm or TPA_ProfileExtensions/xxxx like BusinessTraveler NOTE: PersonName cannot be deleted.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="NewSubtree" type="PartialUpdateProfileType">
            <xs:annotation>
               <xs:documentation>Requires as much data as the view of the client has</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="child" type="PartialChildDataType" use="required"/>
   </xs:complexType>
   <xs:complexType name="ProfileType">
      <xs:annotation>
         <xs:documentation xml:lang="en">Root element for profile content.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="TPA_Identity">
            <xs:annotation>
               <xs:documentation xml:lang="en">***SABRE ADDED*** To hold Identity information of a profile</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:restriction base="TPAIdentityType">
                     <xs:sequence>
                        <xs:element name="Login" type="LoginType" minOccurs="0"/>
                        <xs:element name="ProfileSubType" type="ProfileSubTypeType" minOccurs="0" maxOccurs="5"/>
                        <xs:element name="AuxiliaryID" type="AuxiliaryIDType" minOccurs="0" maxOccurs="5"/>
                     </xs:sequence>
                     <xs:attribute name="ClientCode" type="StringLength1to10" use="required">
                        <xs:annotation>
                           <xs:documentation>ClientCode indicated the code that is associated with the profile that is shared between calling applications</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ClientContextCode" type="StringLength1to3" use="required">
                        <xs:annotation>
                           <xs:documentation>CLientContextCode is the calling application code</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="UniqueID" type="StringLength1to120" use="required"/>
                     <xs:attribute name="ProfileTypeCode" type="ProfileTypeInfo" use="required"/>
                     <xs:attribute name="ProfileName" type="StringLength1to120" use="optional"/>
                     <xs:attribute name="ProfileNameModifyIndicator" type="YesNoType" use="optional" default="Y">
                        <xs:annotation>
                           <xs:documentation xml:lang="en">Indicates if profile name can be modified or not</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ProfileDescription" type="StringLength1to255" use="optional">
                        <xs:annotation>
                           <xs:documentation xml:lang="en">Description of a profile if needed</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="DomainID" type="StringLength1to20" use="required">
                        <xs:annotation>
                           <xs:documentation>DomainID will be PCC for Agency Profiles, SVT for SVT/Inform Profiles</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ProfileStatusCode" type="StatusType" use="optional" default="AC"/>
                     <xs:attribute name="ProfilePurgeNoDays" type="NumericStringLength1to4" use="optional">
                        <xs:annotation>
                           <xs:documentation>Number of days after which the profile should be purged</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:restriction>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:choice minOccurs="0">
            <xs:element name="Traveler" type="TravelerProfileType" minOccurs="0">
               <xs:annotation>
                  <xs:documentation xml:lang="en">***SABRE ADDED*** To hold Traveler Profile Information</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="TravelAgency" type="TravelAgencyProfileType" minOccurs="0">
               <xs:annotation>
                  <xs:documentation xml:lang="en">***SABRE ADDED*** To hold Travel Agency Profile</xs:documentation>
               </xs:annotation>
               <xs:unique name="travelAgencyGDSUniqueness">
                  <xs:selector xpath="./p3:GDS"/>
                  <xs:field xpath="@GDSCode"/>
               </xs:unique>
               <xs:unique name="travelAgencyCustomDefinedValuesNameUniqueness">
                  <xs:selector xpath="./p3:CustomDefinedValues"/>
                  <xs:field xpath="@Name"/>
               </xs:unique>
            </xs:element>
            <xs:element name="TravelAgent" type="TravelAgentProfileType" minOccurs="0">
               <xs:annotation>
                  <xs:documentation xml:lang="en">***SABRE ADDED*** To hold Travel Agent Profile</xs:documentation>
               </xs:annotation>
               <xs:unique name="travelAgentGDSUniqueness">
                  <xs:selector xpath="./p3:GDS"/>
                  <xs:field xpath="@GDSCode"/>
               </xs:unique>
               <xs:unique name="travelAgentCustomDefinedValuesNameUniqueness">
                  <xs:selector xpath="./p3:CustomDefinedValues"/>
                  <xs:field xpath="@Name"/>
               </xs:unique>
            </xs:element>
            <xs:element name="Corporation" type="CorporateProfileType" minOccurs="0">
               <xs:annotation>
                  <xs:documentation xml:lang="en">***SABRE ADDED*** To hold Corporation Profile</xs:documentation>
               </xs:annotation>
               <xs:unique name="corporateGDSUniqueness">
                  <xs:selector xpath="./p3:GDS"/>
                  <xs:field xpath="@GDSCode"/>
               </xs:unique>
               <xs:unique name="corporationCustomDefinedValuesNameUniqueness">
                  <xs:selector xpath="./p3:CustomDefinedValues"/>
                  <xs:field xpath="@Name"/>
               </xs:unique>
            </xs:element>
            <xs:element name="OperationalProfile" type="OperationalProfileType" minOccurs="0">
               <xs:annotation>
                  <xs:documentation xml:lang="en">***SABRE ADDED*** To hold Operational Profiles data</xs:documentation>
               </xs:annotation>
               <xs:unique name="operationalCustomDefinedValuesNameUniqueness">
                  <xs:selector xpath="./p3:CustomDefinedValues"/>
                  <xs:field xpath="@Name"/>
               </xs:unique>
            </xs:element>
            <xs:element name="AllianceProfile" type="AllianceProfileType" minOccurs="0">
               <xs:annotation>
                  <xs:documentation>*** SABRE ADDED *** To hold Alliance Profile data</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="GroupProfile" type="GroupProfileType" minOccurs="0">
               <xs:annotation>
                  <xs:documentation xml:lang="en">***SABRE ADDED*** To hold Corporation Profile</xs:documentation>
               </xs:annotation>
               <xs:unique name="groupProfileGDSUniqueness">
                  <xs:selector xpath="./p3:GDS"/>
                  <xs:field xpath="@GDSCode"/>
               </xs:unique>
               <xs:unique name="groupProfileCustomDefinedValuesNameUniqueness">
                  <xs:selector xpath="./p3:CustomDefinedValues"/>
                  <xs:field xpath="@Name"/>
               </xs:unique>
            </xs:element>
         </xs:choice>
         <xs:choice minOccurs="0">
            <xs:element name="IgnoreSubjectArea" type="IgnoreSubjectAreaType" minOccurs="0">
               <xs:annotation>
                  <xs:documentation xml:lang="en">***SABRE ADDED*** Determines what Subject areas can be loaded along with the webservice operation</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:choice>
         <xs:element name="Association" type="ProfileAssociationType" minOccurs="0">
            <xs:annotation>
               <xs:documentation xml:lang="en">***SABRE ADDED*** To hold associated data identity information</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="CreateDateTime" type="xs:dateTime" use="required">
         <xs:annotation>
            <xs:documentation>*** SABRE ADDED ***</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="UpdateDateTime" type="xs:dateTime" use="required">
         <xs:annotation>
            <xs:documentation>*** SABRE ADDED ***</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="PrimaryLanguageIDCode" type="StringLength1to5" use="optional">
         <xs:annotation>
            <xs:documentation xml:lang="en">Identifes the primary language preference for the message. The human language is identified by ISO 639 codes.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="IgnoreSubjectAreaType">
      <xs:sequence>
         <xs:element name="SubjectAreaName" type="SubjectAreaNamesType" maxOccurs="25"/>
      </xs:sequence>
   </xs:complexType>
