   <xs:complexType name="AssociationType">
      <xs:annotation>
         <xs:documentation xml:lang="en">Root element for Association content.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="AssociationHeaderType">
            <xs:sequence>
               <xs:element name="AssociatedProfiles" type="AssociatedProfilesType" minOccurs="0" maxOccurs="500">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">Information about profiles associated with Association</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="AssociatedFilters" type="TemplateAssociatedFiltersType" minOccurs="0" maxOccurs="100">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">Information about Filters associated with Association</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="AssociatedFormats" type="TemplateAssociatedFormatsType" minOccurs="0" maxOccurs="500">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">Information about Formats associated with Association</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="AssociatedMetadata" type="AssociatedMetadataType" minOccurs="0" maxOccurs="25">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">Information about Metadata associated with Association</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="AssociatedValidators" type="AssociatedValidatorType" minOccurs="0" maxOccurs="25">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">Information about Profile validators associated with Association</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="ProfileAssociationType">
      <xs:complexContent>
         <xs:extension base="AssociationIDType">
            <xs:attribute name="AssociationDescription" type="StringLength1to255" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Description of an association if needed</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="AssociationName" type="StringLength1to120" use="optional">
               <xs:annotation>
                  <xs:documentation>Name of the association</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ClientContextCode" type="StringLength1to3" use="optional">
               <xs:annotation>
                  <xs:documentation>ClientContextCode indicated calling applications ex: MYS, NTC</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ProfileTypeCode" type="ProfileTypeInfo" use="optional">
               <xs:annotation>
                  <xs:documentation>Profile type code, used to validate associated information</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="CreateDateTime" type="xs:dateTime" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Timestamp data, constains information when association was created</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="UpdateDateTime" type="xs:dateTime" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Timestamp data, constains information when association was updated last time</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
</xs:schema>